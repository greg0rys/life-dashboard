<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Finished:</strong>
    <%= book.finished %>
  </p>

  <p>
    <strong>Finishdate:</strong>
    <%= book.finishdate %>
  </p>

  <p>
    <strong>Rate:</strong>
    <%= book.rate %>
  </p>

</div>
